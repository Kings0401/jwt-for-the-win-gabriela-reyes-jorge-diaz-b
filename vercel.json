{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js",
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "headers": {
            "authorization": "Basic <base64-encoded-credentials>"
        }
      }
    ]
  }
  